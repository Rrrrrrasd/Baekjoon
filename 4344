#include <iostream>

using namespace std;

int main()
{
    int n1, n2, num;
    int avg;
    
    int score[1000] = { 0 };
    double result;
    
    cin >> n1;
    for(int i = 0; i< n1; i++)
    {
       avg = 0;
       num = 0;
       cin >> n2;
       
       for(int j = 0; j<n2; j++)
       {
           cin >> score[j];
           avg  = avg + score[j];
       }
       avg = avg / n2;
       
       for(int j = 0; j < n2; j++)
       {
            if(score[j]>avg)
                num++;
       }
       result = (double)num/n2*100;
       
       cout<<fixed;
       cout.precision(3);
       cout<<result << "%" << endl;
       
       
    }
    return   0;
}
